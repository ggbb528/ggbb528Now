{
  "manifest_version": 3,
  "name": "Ggbb528now",
  "version": "1.0.0",
  "description": "勝敗難免開台通知",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Ggbb528now",
    "default_popup": "popup.html"
  },
  "permissions": [
    "alarms",
    "notifications",
    "storage",
    "webNavigation",
    "*://*.twitch.tv/*",
    "*://*.op.gg/"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.twitch.tv/*"],
      "run_at": "document_idle",
      "js": ["contentScript.js"]
    }
  ]
}
